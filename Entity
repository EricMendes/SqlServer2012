            //}
            //Console.WriteLine("Página {0} de {1}. Mostrando {2} registros de um total de {3}", clients.CurrentPage, clients.TotalPages, clients.RegistersPerPage, clients.TotalRegisters);

            Console.ReadKey();
        }
    }
